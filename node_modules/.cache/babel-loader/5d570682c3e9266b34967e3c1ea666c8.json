;AAsBA,IAAA,QAtBA,sBAsBA;AAAA;;AACA,UAAA,KAAA,KAAA,CAAA,IAAA,IAAA,KAAA,KAAA,CAAA,MAAA,EAAA;AACA,aAAA,KAAA,CAAA,UAAA,GAAA,KAAA,UAAA;AACA,QAAA,KAAA,CACA,IADA,CACA,gCADA,EACA,KAAA,KADA,EAEA,IAFA,CAEA,YAAA;AACA;AACA,UAAA,MAAA,CAAA,UAAA,CAAA,GAAA,CAAA,UAAA,IAAA,EAAA;AACA,mBAAA,KAAA,CACA,MADA,CACA,sCAAA,IAAA,CAAA,EADA,EAEA,KAFA,CAEA,UAAA,KAAA;AAAA,qBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,aAFA,CAAA;AAGA,WAJA;;AAKA,UAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,OAAA,CAAA,gBAAA,EAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,QAAA,EAAA,WAFA;AAGA,YAAA,QAAA,EAAA,IAHA;AAIA,YAAA,WAAA,EAAA;AAJA,WAAA;AAMA,SAhBA,EAiBA,KAjBA,CAiBA,UAAA,GAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,SAjBA;AAkBA,OApBA,MAoBA;AACA,aAAA,MAAA,CAAA,KAAA,CAAA,iCAAA,EAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,QAAA,EAAA,WAFA;AAGA,UAAA,QAAA,EAAA,IAHA;AAIA,UAAA,WAAA,EAAA;AAJA,SAAA;AAMA;AACA;AAnDA,GAXA;AAgEA,EAAA,OAhEA,qBAgEA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,kCADA,EAEA,IAFA,CAEA,UAAA,QAAA;AAAA,aAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,IAAA,CAAA;AAAA,KAFA,EAGA,KAHA,CAGA,UAAA,KAAA;AAAA,aAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA;AAAA,KAHA;AAIA,GArEA;AAsEA,EAAA,QAAA,EAAA;AACA,IAAA,UADA,wBACA;AACA,aAAA,KAAA,UAAA,CAAA,MAAA,CAAA,UAAA,KAAA,EAAA,IAAA,EAAA;AACA,eAAA,KAAA,GAAA,IAAA,CAAA,QAAA,CAAA,KAAA,GAAA,IAAA,CAAA,gBAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA;AALA;AAtEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"keranjang\">\r\n    <Navbar :updateKeranjang=\"keranjangs\" />\r\n    <div class=\"container\">\r\n      <!-- breadcrumb -->\r\n      <div class=\"row mt-4\">\r\n        <div class=\"col\">\r\n          <nav aria-label=\"breadcrumb\">\r\n            <ol class=\"breadcrumb\">\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/\" class=\"text-dark\">Beranda</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item\">\r\n                <router-link to=\"/foods\" class=\"text-dark\">Makanan</router-link>\r\n              </li>\r\n              <li class=\"breadcrumb-item active\" aria-current=\"page\">Keranjang</li>\r\n            </ol>\r\n          </nav>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"row\">\r\n        <div class=\"col\">\r\n          <h2>\r\n            Keranjang\r\n            <strong>Saya</strong>\r\n          </h2>\r\n          <div class=\"table-responsive mt-3\">\r\n            <table class=\"table\">\r\n              <thead>\r\n                <tr>\r\n                  <th scope=\"col\">#</th>\r\n                  <th scope=\"col\">Foto</th>\r\n                  <th scope=\"col\">Makanan</th>\r\n                  <th scope=\"col\">Keterangan</th>\r\n                  <th scope=\"col\">Jumlah</th>\r\n                  <th scope=\"col\">Harga</th>\r\n                  <th scope=\"col\">Total Harga</th>\r\n                  <th scope=\"col\">Hapus</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(keranjang, index) in keranjangs\" :key=\"keranjang.id\">\r\n                  <th>{{index+1}}</th>\r\n                  <td>\r\n                    <img\r\n                      :src=\" '../assets/images/' + keranjang.products.gambar \"\r\n                      class=\"img-fluid shadow\"\r\n                      width=\"250\"\r\n                    />\r\n                  </td>\r\n                  <td>\r\n                    <strong>{{ keranjang.products.nama }}</strong>\r\n                  </td>\r\n                  <td>{{ keranjang.keterangan ? keranjang.keterangan : \"-\" }}</td>\r\n                  <td>{{ keranjang.jumlah_pemesanan }}</td>\r\n                  <td align=\"right\">Rp. {{ keranjang.products.harga }}</td>\r\n                  <td align=\"right\">\r\n                    <strong>Rp. {{ keranjang.products.harga*keranjang.jumlah_pemesanan }}</strong>\r\n                  </td>\r\n                  <td align=\"center\" class=\"text-danger\">\r\n                    <b-icon-trash @click=\"hapusKeranjang(keranjang.id)\"></b-icon-trash>\r\n                  </td>\r\n                </tr>\r\n\r\n                <tr>\r\n                  <td colspan=\"6\" align=\"right\">\r\n                    <strong>Total Harga :</strong>\r\n                  </td>\r\n                  <td align=\"right\">\r\n                    <strong>Rp. {{ totalHarga }}</strong>\r\n                  </td>\r\n                  <td></td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Form Checkout -->\r\n      <div class=\"row justify-content-end\">\r\n        <div class=\"col-md-4\">\r\n          <form class=\"mt-4\" v-on:submit.prevent>\r\n            <div class=\"form-group\">\r\n              <label for=\"nama\">Nama :</label>\r\n              <input type=\"text\" class=\"form-control\" v-model=\"pesan.nama\" />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"noMeja\">Nomor Meja :</label>\r\n              <input type=\"text\" class=\"form-control\" v-model=\"pesan.noMeja\" />\r\n            </div>\r\n\r\n            <button type=\"submit\" class=\"btn btn-success float-right\" @click=\"checkout\">\r\n              <b-icon-cart></b-icon-cart>Pesan\r\n            </button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Navbar from \"@/components/Navbar.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"Keranjang\",\r\n  components: {\r\n    Navbar,\r\n  },\r\n  data() {\r\n    return {\r\n      keranjangs: [],\r\n      pesan: {},\r\n    };\r\n  },\r\n  methods: {\r\n    setKeranjangs(data) {\r\n      this.keranjangs = data;\r\n    },\r\n    hapusKeranjang(id) {\r\n      axios\r\n        .delete(\"http://localhost:3000/keranjangs/\" + id)\r\n        .then(() => {\r\n          this.$toast.error(\"Sukses Hapus Keranjang\", {\r\n            type: \"error\",\r\n            position: \"top-right\",\r\n            duration: 3000,\r\n            dismissible: true,\r\n          });\r\n          // Update Data keranjang\r\n          axios\r\n            .get(\"http://localhost:3000/keranjangs\")\r\n            .then((response) => this.setKeranjangs(response.data))\r\n            .catch((error) => console.log(error));\r\n        })\r\n        .catch((error) => console.log(error));\r\n    },\r\n    checkout() {\r\n      if (this.pesan.nama && this.pesan.noMeja) {\r\n        this.pesan.keranjangs = this.keranjangs;\r\n        axios\r\n          .post(\"http://localhost:3000/pesanans\", this.pesan)\r\n          .then(() => {\r\n            // Hapus Semua Keranjang \r\n            this.keranjangs.map(function (item) {\r\n              return axios\r\n                .delete(\"http://localhost:3000/keranjangs/\" + item.id)\r\n                .catch((error) => console.log(error));\r\n            });\r\n            this.$router.push({ path: \"/pesanan-sukses\" });\r\n            this.$toast.success(\"Sukses Dipesan\", {\r\n              type: \"success\",\r\n              position: \"top-right\",\r\n              duration: 3000,\r\n              dismissible: true,\r\n            });\r\n          })\r\n          .catch((err) => console.log(err));\r\n      } else {\r\n        this.$toast.error(\"Nama dan Nomor Meja Harus diisi\", {\r\n          type: \"error\",\r\n          position: \"top-right\",\r\n          duration: 3000,\r\n          dismissible: true,\r\n        });\r\n      }\r\n    },\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:3000/keranjangs\")\r\n      .then((response) => this.setKeranjangs(response.data))\r\n      .catch((error) => console.log(error));\r\n  },\r\n  computed: {\r\n    totalHarga() {\r\n      return this.keranjangs.reduce(function (items, data) {\r\n        return items + data.products.harga * data.jumlah_pemesanan;\r\n      }, 0);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/views"}]}